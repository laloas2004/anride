 <%- include ../includes/menu %>

<div class="container">

<div class="row">
<div class ="col-md-12">
<div class="page-header">
<h1>Clientes</h1>
<div>
<div class=""><a href="/admin/clientes/new" class="btn btn-default" id="btn_cliente">Nuevo Cliente</a></div>
<table class="table table-striped">
    <thead>
      <tr>
        
        <th>Nombre</th>
        <th>Apellidos</th>
        <th>Email</th>
        <th>Telefono</th>
        <th>Operaciones</th>
      </tr>
    </thead>
<tbody>
 <%  

  _.each(clientes,function(cliente){ %>
      <tr>
        
        <td><%= cliente.nombre%></td>
        <td><%= cliente.apellido%></td>
        <td><%= cliente.email%></td>
        <td><%= cliente.numCel%></td>
        <td><a href="#" class="btn btn-primary">Editar</a> <a href="#" class="btn btn-danger">Eliminar</a></td>
      </tr>
<% })%>
</tbody>
    </table>

</div>
</div>


</div>